<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Proyecto Tech</title>
</head>
<body>
    <%- include('partials/videoBackground') %>

    <div class="mainContainer">
        <div class="header">
            <button class="buttonHome" id="buttonHome">
                <img src="/img/homeIcon.png" alt="galeryIcon">
            </button>
            <div class="title">
                <h1>Galeria</h1>
            </div>
        </div>
        <div class="galeryContainer">
            <% data.forEach(birdData => { %>
                <%- include('partials/buttonGalery', { birdData, birdName: birdData.especie.replace(" ", "-") }); %>
            <% }); %>
        </div>
    </div>
    <link rel="stylesheet" href="/css/galery.css">
    <script src="/js/galery.js"></script>
</body>
</html>